{
	"name": "typescript-node-module-boilerplate",
	"version": "0.0.0",
	"description": "",
	"license": "MIT",
	"repository": "<%= githubUsername %>/typescript-node-module-boilerplate",
	"author": {
		"name": "<%= name %>",
		"email": "<%= email %>",
		"url": "<%= website %>"
	},
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"build": "del-cli dist && tsc",
		"prepack": "npm run build",
		"test": "tsc --sourceMap && xo && nyc ava"
	},
	"main": "dist/source",
	"types": "dist/source",
	"files": [
		"dist/source",
		"!*.test.*"
	],
	"keywords": [
		""
	],
	"dependencies": {},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/node": "^13.1.4",
		"ava": "^3.0.0",
		"del-cli": "^3.0.0",
		"nyc": "^15.0.0",
		"typescript": "^3.7.4",
		"xo": "^0.28.3"
	},
	"nyc": {
		"all": true,
		"extension": [
			".ts"
		],
		"include": [
			"dist",
			"source"
		],
		"exclude": [
			"test/**",
			"**/*.d.ts",
			"**/*.test.ts"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"xo": {
		"rules": {
			"ava/no-ignored-test-files": "off"
		}
	}
}
